{
  "name": "stayfithq",
  "version": "1.0.0",
  "description": "Health & Fitness Dashboard - AWS Cloud Ready Application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node run-localhost.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "node scripts/build-server.js",
    "deploy": "npm run build && node scripts/deploy.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix"
  },
  "keywords": [
    "health",
    "fitness",
    "dashboard",
    "aws",
    "cloud",
    "nodejs",
    "healthcare"
  ],
  "author": "StayFitHQ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/schinchli/StayFitHQ.git"
  },
  "bugs": {
    "url": "https://github.com/schinchli/StayFitHQ/issues"
  },
  "homepage": "https://github.com/schinchli/StayFitHQ#readme",
  "dependencies": {
    "@opensearch-project/opensearch": "^3.5.1",
    "@vendia/serverless-express": "^4.12.6",
    "aws-sdk": "^2.1490.0",
    "aws-xray-sdk-core": "^3.5.1",
    "aws-xray-sdk-express": "^3.5.1",
    "aws-xray-sdk-mysql": "^3.5.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "clean-css": "^5.3.3",
    "html-minifier-terser": "^7.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "terser": "^5.43.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
