# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id
aws_access_key_id = YOUR_AWS_ACCESS_KEY
aws_secret_access_key = YOUR_AWS_SECRET_KEY

# Application Configuration
NODE_ENV=development
PORT=3000
ALLOWED_ORIGINS=http://localhost:3000,https://d3r155fcnafufg.cloudfront.net

# X-Ray Configuration
_X_AMZN_TRACE_ID=auto
AWS_XRAY_TRACING_NAME=stayfit-health-companion
AWS_XRAY_DEBUG_MODE=false
AWS_XRAY_CONTEXT_MISSING=LOG_ERROR

# CloudTrail Configuration
CLOUDTRAIL_ENABLED=true
CLOUDWATCH_LOGS_ENABLED=true
LOG_LEVEL=info

# Health Data Configuration
HEALTH_DATA_ENCRYPTION=true
AUDIT_LOGGING=true
COMPLIANCE_MODE=HIPAA

# AI Configuration
AI_SERVICE_ENDPOINT=https://bedrock-runtime.us-east-1.amazonaws.com
AI_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
AI_MAX_TOKENS=1000

# Security Configuration
SESSION_SECRET=your-session-secret
ENCRYPTION_KEY=your-encryption-key
JWT_SECRET=your-jwt-secret
