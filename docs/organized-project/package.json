{
  "name": "stayfit-health-companion",
  "version": "1.0.0",
  "description": "Digital Health Companion with AI-powered insights, real-time health data visualization, and comprehensive AWS observability",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "build": "npm run build:css && npm run build:js && npm run build:html",
    "build:css": "npm run minify:css",
    "build:js": "npm run minify:js",
    "build:html": "npm run minify:html",
    "minify:css": "node scripts/minify-css.js",
    "minify:js": "node scripts/minify-js.js",
    "minify:html": "node scripts/minify-html.js",
    "minify:all": "npm run minify:css && npm run minify:js && npm run minify:html",
    "deploy": "npm run build && node scripts/deploy.js",
    "setup:xray": "node scripts/setup-xray.js",
    "setup:cloudtrail": "node scripts/setup-cloudtrail.js",
    "setup:monitoring": "npm run setup:xray && npm run setup:cloudtrail",
    "lint": "echo 'Linting placeholder'",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^3.5.1",
    "@vendia/serverless-express": "^4.12.6",
    "aws-sdk": "^2.1490.0",
    "aws-xray-sdk-core": "^3.5.1",
    "aws-xray-sdk-express": "^3.5.1",
    "aws-xray-sdk-mysql": "^3.5.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "clean-css": "^5.3.3",
    "html-minifier-terser": "^7.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "terser": "^5.43.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/stayfit-health-companion.git"
  },
  "keywords": [
    "health",
    "ai",
    "aws",
    "dashboard",
    "analytics",
    "mcp",
    "xray",
    "cloudtrail",
    "observability",
    "minified",
    "optimized"
  ],
  "author": "Shashank Chinchli",
  "license": "MIT"
}
